{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "type:check": {
      "outputs": ["*.tsbuildinfo"]
    },
    "biome:check": {
      "outputs": [],
      "inputs": ["biome.json", "src/**"]
    },
    "build": {
      "dependsOn": ["type:check", "biome:check"],
      "outputs": ["dist/**", "public/**"],
      "env": []
    }
  }
}
